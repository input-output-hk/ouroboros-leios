{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "anyOf": [
    {
      "$ref": "#/definitions/RustCpuEvent"
    },
    {
      "$ref": "#/definitions/RustBlockEvent"
    },
    {
      "$ref": "#/definitions/RustTransactionEvent"
    },
    {
      "$ref": "#/definitions/RustVoteEvent"
    },
    {
      "$ref": "#/definitions/RustSlotEvent"
    }
  ],
  "definitions": {
    "Record<string,number>": {
      "type": "object"
    },
    "RustBaseBlockEvent": {
      "properties": {
        "id": {
          "type": "string"
        },
        "pipeline": {
          "type": "number"
        },
        "producer": {
          "type": "string"
        },
        "recipient": {
          "type": "string"
        },
        "sender": {
          "type": "string"
        },
        "slot": {
          "type": "number"
        }
      },
      "required": ["id", "producer", "slot"],
      "type": "object"
    },
    "RustBlockEvent": {
      "properties": {
        "message": {
          "allOf": [
            {
              "$ref": "#/definitions/RustBaseBlockEvent"
            },
            {
              "properties": {
                "endorsement": {},
                "header_bytes": {
                  "type": "number"
                },
                "index": {
                  "type": "number"
                },
                "parent": {
                  "properties": {
                    "id": {
                      "type": "string"
                    },
                    "producer": {
                      "type": "string"
                    },
                    "slot": {
                      "type": "number"
                    }
                  },
                  "required": ["id", "producer", "slot"],
                  "type": "object"
                },
                "size_bytes": {
                  "type": "number"
                },
                "transactions": {
                  "items": {
                    "type": "string"
                  },
                  "type": "array"
                },
                "type": {
                  "$ref": "#/definitions/RustBlockMessageType"
                },
                "vrf": {
                  "type": "number"
                }
              },
              "required": ["type"],
              "type": "object"
            }
          ]
        },
        "time_s": {
          "type": "number"
        }
      },
      "required": ["message", "time_s"],
      "type": "object"
    },
    "RustBlockMessageType": {
      "enum": [
        "EBGenerated",
        "EBLotteryWon",
        "EBReceived",
        "EBSent",
        "IBGenerated",
        "IBLotteryWon",
        "IBReceived",
        "IBSent",
        "RBGenerated",
        "RBLotteryWon",
        "RBReceived",
        "RBSent"
      ],
      "type": "string"
    },
    "RustCpuEvent": {
      "properties": {
        "message": {
          "properties": {
            "cpu_time_s": {
              "type": "number"
            },
            "duration_s": {
              "type": "number"
            },
            "extra": {
              "type": "string"
            },
            "subtask_id": {
              "type": "number"
            },
            "subtasks": {
              "type": "number"
            },
            "task": {
              "$ref": "#/definitions/RustTaskInfo"
            },
            "task_type": {
              "enum": [
                "EBBlockGenerated",
                "EBBlockValidated",
                "IBBlockGenerated",
                "IBBlockValidated",
                "IBHeaderGenerated",
                "IBHeaderValidated",
                "RBBlockGenerated",
                "RBBlockValidated",
                "TransactionGenerated",
                "TransactionValidated",
                "VTBundleGenerated",
                "VTBundleValidated"
              ],
              "type": "string"
            },
            "type": {
              "$ref": "#/definitions/RustCpuMessageType"
            },
            "wall_time_s": {
              "type": "number"
            }
          },
          "required": ["task", "type"],
          "type": "object"
        },
        "time_s": {
          "type": "number"
        }
      },
      "required": ["message", "time_s"],
      "type": "object"
    },
    "RustCpuMessageType": {
      "enum": [
        "CpuSubtaskFinished",
        "CpuSubtaskScheduled",
        "CpuSubtaskStarted",
        "CpuTaskFinished",
        "CpuTaskScheduled",
        "CpuTaskStarted"
      ],
      "type": "string"
    },
    "RustSlotEvent": {
      "properties": {
        "message": {
          "properties": {
            "number": {
              "type": "number"
            },
            "type": {
              "const": "Slot",
              "type": "string"
            }
          },
          "required": ["number", "type"],
          "type": "object"
        },
        "time_s": {
          "type": "number"
        }
      },
      "required": ["message", "time_s"],
      "type": "object"
    },
    "RustTaskInfo": {
      "properties": {
        "index": {
          "type": "number"
        },
        "node": {
          "type": "string"
        }
      },
      "required": ["index", "node"],
      "type": "object"
    },
    "RustTransactionEvent": {
      "properties": {
        "message": {
          "properties": {
            "bytes": {
              "type": "number"
            },
            "id": {
              "type": "string"
            },
            "publisher": {
              "type": "string"
            },
            "recipient": {
              "type": "string"
            },
            "sender": {
              "type": "string"
            },
            "type": {
              "$ref": "#/definitions/RustTransactionMessageType"
            }
          },
          "required": ["id", "type"],
          "type": "object"
        },
        "time_s": {
          "type": "number"
        }
      },
      "required": ["message", "time_s"],
      "type": "object"
    },
    "RustTransactionMessageType": {
      "enum": [
        "TransactionGenerated",
        "TransactionReceived",
        "TransactionSent"
      ],
      "type": "string"
    },
    "RustVoteEvent": {
      "properties": {
        "message": {
          "properties": {
            "id": {
              "type": "string"
            },
            "producer": {
              "type": "string"
            },
            "recipient": {
              "type": "string"
            },
            "sender": {
              "type": "string"
            },
            "slot": {
              "type": "number"
            },
            "type": {
              "$ref": "#/definitions/RustVoteMessageType"
            },
            "votes": {
              "$ref": "#/definitions/Record<string,number>"
            }
          },
          "required": ["id", "producer", "slot", "type"],
          "type": "object"
        },
        "time_s": {
          "type": "number"
        }
      },
      "required": ["message", "time_s"],
      "type": "object"
    },
    "RustVoteMessageType": {
      "enum": [
        "VotesGenerated",
        "VotesLotteryWon",
        "VotesReceived",
        "VotesSent"
      ],
      "type": "string"
    }
  }
}
